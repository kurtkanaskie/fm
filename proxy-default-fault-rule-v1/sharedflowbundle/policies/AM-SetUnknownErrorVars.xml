<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-SetUnknownErrorVars">
    <DisplayName>AM-SetUnknownErrorVars</DisplayName>
    <AssignVariable>
        <Name>fm.error.status</Name>
        <Value>500</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>fm.error.reason</Name>
        <Value>Unknown Error</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>fm.error.code</Name>
        <Value>500.001</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>fm.error.description</Name>
        <Value>Something went wrong in the backend</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>fm.error.detailsarray</Name>
        <Value>[]</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>