        <Headers>
            <Header name="X-Forwarded-ProxyMessageId">{messageid}</Header>
            <Header name="X-Forwarded-UserName">{accesstoken.UserName}</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <DisplayName>AM-AssignHeaders</DisplayName>
</AssignMessage>